{
  "name": "ngm",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint src/ --ext .ts,.js",
    "extract-from-assets": "node scripts/get_assets_properties.js src/assets_properties.js",
    "extract-i18n": "i18next -c i18next-parser.config.cjs",
    "start": "npm run webpack-start",
    "build-static": "node scripts/environment.js $RELEASE_NAME && node scripts/generateManuals.js && bash scripts/legal_md_to_html.sh",
    "webpack-start": "npm run build-static && webpack serve",
    "rollup-start": "npm run build-static && rollup --environment SERVE:true -c --watch --no-treeshake",
    "build": "rm -rf dist/* && npm run build-static && rollup --environment mode:production -c && bash scripts/create_versions.sh",
    "check": "npm run check-main && npm run check-ut",
    "check-main": "node_modules/.bin/tsc src/index.ts --noEmit --allowJs --skipLibCheck --target es2018 --moduleResolution node",
    "check-ut": "node_modules/.bin/tsc src/test/*.ts src/test/*.js  --noEmit --allowJs --skipLibCheck --esModuleInterop true --target es2018 --moduleResolution node",
    "ut": "mocha --exit --experimental-specifier-resolution=node",
    "test": "npm run ut && npm run check",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "start-server-and-test start http-get://localhost:8000 cypress:run"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "^3.31.0",
    "@aws-sdk/client-s3": "^3.31.0",
    "@aws-sdk/credential-provider-cognito-identity": "^3.31.0",
    "@aws-sdk/s3-request-presigner": "^3.31.0",
    "@geoblocks/cesium-compass": "0.3.0",
    "@geoblocks/cesium-view-cube": "0.0.1",
    "@geoblocks/ga-search": "0.0.20",
    "@sentry/browser": "^6.12.0",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "cesium": "^1.85.0",
    "event-target-shim": "^6.0.2",
    "fomantic-ui-css": "^2.8.8",
    "i18next": "^20.6.1",
    "i18next-xhr-backend": "^3.2.2",
    "interactjs": "^1.10.11",
    "jquery": "^3.6.0",
    "jszip": "^3.7.1",
    "lit-element": "^3.0.1",
    "lit-html": "^1.4.1",
    "loc-i18next": "^0.1.4",
    "papaparse": "^5.3.1",
    "proj4": "^2.7.5",
    "rxjs": "^7.3.0",
    "typeface-source-sans-pro": "1.1.13"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.17.17",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "autoprefixer": "^10.3.4",
    "babel-loader": "^8.2.2",
    "chai": "^4.3.4",
    "copy-webpack-plugin": "^9.0.1",
    "core-js": "^3.17.3",
    "css-loader": "^6.2.0",
    "cypress": "^8.4.1",
    "eslint": "^7.32.0",
    "eslint-plugin-cypress": "^2.12.1",
    "file-loader": "^6.2.0",
    "file-saver": "^2.0.5",
    "i18next-parser": "^4.7.0",
    "jsdom-global": "^3.0.2",
    "marked": "^3.0.4",
    "mini-css-extract-plugin": "^2.3.0",
    "mocha": "^9.1.1",
    "node-fetch": "^3.0.0",
    "postcss": "^8.3.6",
    "postcss-import": "^14.0.2",
    "postcss-inline-svg": "^5.0.0",
    "postcss-url": "^10.1.3",
    "rollup": "^2.56.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-ejs": "^2.2.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-strip-pragma": "^1.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "source-map-loader": "^3.0.0",
    "start-server-and-test": "^1.14.0",
    "stream-to-promise": "^3.0.0",
    "ts-loader": "^9.2.5",
    "ts-mocha": "^8.0.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.1"
  }
}
