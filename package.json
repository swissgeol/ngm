    "@geoblocks/cesium-view-cube": "0.0.1",
    "@geoblocks/ga-search": "0.0.20",
    "@sentry/browser": "^6.8.0",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "cesium": "^1.82.1",
    "event-target-shim": "^6.0.2",
    "fomantic-ui-css": "^2.8.8",
    "i18next": "^20.3.2",
    "i18next-xhr-backend": "^3.2.2",
    "interactjs": "^1.10.11",
    "jquery": "^3.6.0",
    "jszip": "^3.6.0",
    "lit-element": "^2.5.1",
