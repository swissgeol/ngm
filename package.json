{
  "name": "ngm",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint src/ --ext .ts,.js",
    "extract-from-assets": "node scripts/get_assets_properties.js src/assets_properties.js",
    "extract-i18n": "i18next -c i18next-parser.config.cjs",
    "start": "npm run webpack-start",
    "build-static": "node scripts/environment.js $RELEASE_NAME && node scripts/generateManuals.js && bash scripts/legal_md_to_html.sh",
    "webpack-start": "npm run build-static && webpack serve",
    "rollup-start": "npm run build-static && rollup --environment SERVE:true -c --watch --no-treeshake",
    "build": "rm -rf dist/* && npm run build-static && rollup --environment mode:production -c && bash scripts/create_versions.sh; find dist -type l -exec rm {} +",
    "check": "npm run check-main && npm run check-ut",
    "check-main": "node_modules/.bin/tsc src/index.ts --noEmit --allowJs --skipLibCheck --target es2018 --moduleResolution node --experimentalDecorators",
    "check-ut": "node_modules/.bin/tsc src/test/*.ts src/test/*.js  --noEmit --allowJs --skipLibCheck --esModuleInterop true --target es2018 --moduleResolution node --experimentalDecorators",
    "ut": "mocha --exit --experimental-specifier-resolution=node -r jsdom-global/register -r src/test/jquery.register.js",
    "test": "npm run ut && npm run check",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --browser chrome",
    "test:e2e": "start-server-and-test start http-get://localhost:8000 cypress:run",
    "storybook": "start-storybook -p 6006 --modern --static-dir .storybook/static",
    "build-storybook": "build-storybook && cp -R locales storybook-static && cp -R node_modules/cesium/Build/Cesium/Assets node_modules/cesium/Build/Cesium/Widgets node_modules/cesium/Build/Cesium/Workers storybook-static"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "^3.43.0",
    "@aws-sdk/client-s3": "^3.44.0",
    "@aws-sdk/credential-provider-cognito-identity": "^3.43.0",
    "@aws-sdk/s3-request-presigner": "^3.44.0",
    "@fontsource/inter": "^4.5.1",
    "@geoblocks/cesium-view-cube": "0.1.0",
    "@geoblocks/ga-search": "0.0.20",
    "@sentry/browser": "^6.16.1",
    "cesium": "^1.88.0",
    "earcut": "^2.2.3",
    "event-target-shim": "^6.0.2",
    "fomantic-ui-css": "^2.8.8",
    "i18next": "^21.6.0",
    "i18next-xhr-backend": "^3.2.2",
    "interactjs": "^1.10.11",
    "jquery": "^3.6.0",
    "jszip": "^3.7.1",
    "lit": "^2.0.2",
    "loc-i18next": "^0.1.4",
    "papaparse": "^5.3.1",
    "proj4": "^2.7.5",
    "rxjs": "^7.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/plugin-proposal-decorators": "^7.16.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@sentry/cli": "^1.71.0",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/builder-webpack5": "^6.4.9",
    "@storybook/manager-webpack5": "^6.4.9",
    "@storybook/web-components": "^6.4.9",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.7.0",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "chai": "^4.3.4",
    "copy-webpack-plugin": "^10.2.0",
    "core-js": "^3.20.0",
    "css-loader": "^6.5.1",
    "cypress": "^9.1.1",
    "eslint": "^8.4.1",
    "eslint-plugin-cypress": "^2.12.1",
    "file-loader": "^6.2.0",
    "file-saver": "^2.0.5",
    "i18next-parser": "^5.3.0",
    "jsdom": "^19.0.0",
    "jsdom-global": "^3.0.2",
    "marked": "^4.0.7",
    "mini-css-extract-plugin": "^2.4.5",
    "mocha": "^9.1.3",
    "node-fetch": "^3.1.0",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "postcss-inline-svg": "^5.0.0",
    "postcss-url": "^10.1.3",
    "rollup": "^2.61.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-strip-pragma": "^1.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "source-map-loader": "^3.0.0",
    "start-server-and-test": "^1.14.0",
    "stream-to-promise": "^3.0.0",
    "ts-loader": "^9.2.6",
    "ts-mocha": "^8.0.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.2"
  }
}
